{
    "explorer.confirmDragAndDrop": false,
    "explorer.confirmDelete": false,
    "git.confirmSync": false,
    "git.autofetch": true,
    "workbench.sideBar.location": "right",
    "terminal.integrated.fontFamily": "'JetBrainsMono Nerd Font Mono'",
    "editor.fontFamily": "'JetBrainsMono Nerd Font Mono'",
    "[dockercompose]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 2,
        "editor.autoIndent": "advanced",
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "[github-actions-workflow]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "files.autoSave": "onFocusChange",
    "python.analysis.typeCheckingMode": "standard",
    "python.analysis.typeEvaluation.deprecateTypingAliases": true,
    "python.analysis.typeEvaluation.enableTypeIgnoreComments": false,
    "github.copilot.nextEditSuggestions.enabled": true,
    "workbench.colorTheme": "Monokai Night",

    "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.formatOnSave": true,
    "diffEditor.ignoreTrimWhitespace": false
},
"github.copilot.enable": {
    "*": false,
    "plaintext": false,
    "markdown": false,
    "scminput": false,
    "python": false
},
"psi-header.config": {
  "author": "Austin Delic",
  "authorEmail": "austin@austindelic.com"
},
"chat.tools.terminal.autoApprove": {
    "cd /Users/AustinDe/projects/adventure-world && python3 -c \"\nfrom src.cli import cli\nfrom click.testing import CliRunner\nrunner = CliRunner()\nresult = runner.invoke(cli, ['-f', 'examples/scenario1.json'])\nprint(result.output[:500])\nif result.exception:\n    import traceback\n    traceback.print_exception(type(result.exception), result.exception, result.exception.__traceback__)\n\" 2>&1": {
        "approve": true,
        "matchCommandLine": true
    },
    "python3 test_patron_behavior.py": {
        "approve": true,
        "matchCommandLine": true
    }
}
}